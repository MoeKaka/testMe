SELECT 
    t.table_name,
    t.num_rows,
    CASE 
        WHEN t.num_rows = 0 OR t.num_rows IS NULL THEN 'Empty'
        ELSE 'Not Empty'
    END AS table_status,
    t.last_analyzed
FROM all_tables t
WHERE t.owner = 'KAKA'
ORDER BY 
    CASE 
        WHEN t.num_rows = 0 OR t.num_rows IS NULL THEN 1
        ELSE 2
    END,
    t.table_name;
